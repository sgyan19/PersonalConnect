<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <CheckBoxPreference
        android:defaultValue="false"
        android:key="status"
        android:summaryOff="settings_status_off_summary"
        android:summaryOn="settings_status_on_summary"
        android:title="settings_status_title" />

    <EditTextPreference
        android:key="id"
        android:title="settings_id_title" />

    <EditTextPreference
        android:defaultValue="demo.traccar.org"
        android:key="address"
        android:summary="settings_address_summary"
        android:title="settings_address_title" />

    <EditTextPreference
        android:defaultValue="5055"
        android:key="port"
        android:numeric="integer"
        android:summary="settings_port_summary"
        android:title="settings_port_title" />

    <CheckBoxPreference
        android:defaultValue="false"
        android:key="secure"
        android:summaryOff="settings_secure_off_summary"
        android:summaryOn="settings_secure_on_summary"
        android:title="settings_secure_title" />

    <EditTextPreference
        android:defaultValue="300"
        android:key="interval"
        android:numeric="integer"
        android:summary="settings_interval_summary"
        android:title="settings_interval_title" />

    <ListPreference
        android:defaultValue="gps"
        android:key="provider"
        android:summary="settings_provider_summary"
        android:title="settings_provider_title" />

    <!--android:entries="@array/settings_provider_names"-->
    <!--android:entryValues="@array/settings_provider_values"-->
</PreferenceScreen>
